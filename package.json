{
  "name": "dynamark",
  "version": "0.1.0",
  "description": "A CLI to render Handlebars markdown templates with data content",
  "bin": "./dist/index.js",
  "scripts": {
    "build": "rm -rf dist/ && npx tsc --build src/ && chmod +x dist/index.js",
    "process": "ts-node --transpile-only --log-error src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tobilg/dynamark.git"
  },
  "keywords": [
    "markdown",
    "template",
    "rendering",
    "cli",
    "automation"
  ],
  "author": "TobiLG <tobilg@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tobilg/dynamark/issues"
  },
  "homepage": "https://github.com/tobilg/dynamark#readme",
  "devDependencies": {
    "@types/node": "^20.1.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "commander": "^10.0.1",
    "handlebars": "^4.7.7",
    "markdownlint": "^0.28.2",
    "yaml": "^2.3.1"
  }
}
